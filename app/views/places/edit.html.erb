<div class="container">
    <div class="hooyah-box col-12 col-md-6 offset-md-3">
        <h1>Edit Your Place</h1>
        <%= simple_form_for @place do |f| %>
            <%= f.input :name %>
            <%= f.input :address %>
            <%= f.input :description %>
            <br />
            <%= f.submit 'Update', class: 'class="btn btn-outline-primary' %>
        <% end %>
    </div>
</div>
